              "margin-top": "0",
              "margin-bottom": "0",
              "margin-right": "0",
              "padding-top": "0.2rem",
              "padding-left": "0",
              "padding-right": "0.5rem",
              "padding-bottom": "0.2rem"
            },
            ".topLineDivSettings": {
              "z-index": "1",
              "width": "100%",
              "padding-left": "0",
              "padding-right": "0",
              "border-style": "solid",
              "border-top-width": "(@TopLineULTopThicknessPX * 1px)",
              "border-bottom-width": "(@TopLineULBottomThicknessPX * 1px)",
              "border-left-width": "0",
              "border-right-width": "0",
              "border-bottom-color": "@TopLineULBottomCol",
