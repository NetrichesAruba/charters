          "_customHTML": "<section data-bs-version=\"5.1\" class=\"footer4\" group=\"Footers\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" global once=\"footers\" mbr-class=\"{\n    'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"3\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"3\">\n        <input type=\"range\" inline title=\"Logo Size\" name=\"logoSize\" min=\"1\" max=\"3\" step=\"1\" value=\"2\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Social\" name=\"showSocial\" checked>\n        <input type=\"range\" name=\"socialCount\" title=\"Social Count\" min=\"1\" max=\"4\" step=\"1\" value=\"4\" condition=\"showSocial\">\n        <input type=\"checkbox\" title=\"Copyright\" name=\"showCopyright\" checked>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background8.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#232323\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#232323\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row mbr-white\">\n            <div class=\"col-6 col-lg-3\">\n                <div class=\"media-wrap col-md-{{4*logoSize}} col-12\">\n                    <a href=\"index.html\" target=\"_blank\">\n                        <img src=\"@PROJECT_PATH@/assets/images/zeorita-exclusive-charters-aruba-logo-menu.webp\" alt=\"Zeñorita Exclusive Charters logo\">\n                    </a>\n                </div>\n            </div>\n            <div class=\"col-12 col-md-6 col-lg-3\">\n                <h5 class=\"mbr-section-subtitle mbr-fonts-style mb-2\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\">Zeñorita Exclusive Charters</h5>\n                <p class=\"mbr-text mbr-fonts-style mb-4\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">If you want a charter, don't hesitate to contact us!\n<br><br>Address:\n<br><a href=\"index.html\" class=\"text-primary\" target=\"_blank\">Oranjestad, Aruba</a><br><br>Email:&nbsp;<a href=\"mailto:boataruba@gmail.com\" class=\"text-primary\">boataruba@gmail.com</a><br><br>Phone:&nbsp;<a href=\"https://wa.me/297 123 4567\" class=\"text-primary\">+297 123 4567</a><a href=\"http://exclusivearuba.online/\" class=\"text-white\" target=\"_blank\"><br></a></p>\n                <h5 class=\"mbr-section-subtitle mbr-fonts-style mb-3\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-section-subtitle\" mbr-if=\"showTitle && showSocial\">Connect with Us</h5>\n                <div class=\"social-row display-7\" mbr-if=\"showSocial\">\n                    <div class=\"soc-item\">\n                        <a href=\"index.html\" target=\"_blank\">\n                            <span mbr-icon class=\"mbr-iconfont fa-facebook-square fa\" style=\"font-size: 22px; color: rgb(68, 121, 217); fill: rgb(68, 121, 217);\"></span>\n                        </a>\n                    </div>\n                    <div class=\"soc-item\" mbr-if=\"socialCount>1\">\n                        <a href=\"index.html\" target=\"_blank\">\n                            <span mbr-icon class=\"mbr-iconfont socicon-twitter socicon\" style=\"color: rgb(68, 121, 217); fill: rgb(68, 121, 217);\"></span>\n                        </a>\n                    </div>\n                    <div class=\"soc-item\" mbr-if=\"socialCount>2\">\n                        <a href=\"index.html\" target=\"_blank\">\n                            <span mbr-icon class=\"mbr-iconfont socicon-instagram socicon\" style=\"font-size: 22px; color: rgb(68, 121, 217); fill: rgb(68, 121, 217);\"></span>\n                        </a>\n                    </div>\n                    <div class=\"soc-item\" mbr-if=\"socialCount>3\">\n                        <a href=\"index.html\" target=\"_blank\">\n                            <span mbr-icon class=\"mbr-iconfont socicon-youtube socicon\" style=\"color: rgb(68, 121, 217); fill: rgb(68, 121, 217);\"></span>\n                        </a>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-md-6 col-lg-3\">\n                <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style mb-2\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\"><a href=\"Aruba-Yacht-Charter-Events-Tours.html\" class=\"text-primary\">Boat trips on Aruba</a></h5>\n                <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline mbr-article>\n                    <li class=\"mbr-text item-wrap\"><a href=\"Aruba-Yacht-Charter-Events-Tours.html\" class=\"text-white\" style=\"font-size: 1.1rem;\">Event Yacht Charters</a><br></li><li class=\"mbr-text item-wrap\"><a href=\"Daily-Yacht-Charters-Aruba.html\" class=\"text-white\">Daily Yacht Charter</a></li><li class=\"mbr-text item-wrap\"><a href=\"Private-Fishing-Charter-Aruba.html\" class=\"text-white\">Private Fishing Charter</a></li><li class=\"mbr-text item-wrap\"><a href=\"yacht-charter-catering.html\" class=\"text-primary\"></a><a href=\"yacht-charter-catering.html\" class=\"text-white text-primary\">Yacht Charter Catering</a></li><li class=\"mbr-text item-wrap\"><a href=\"Yacht-Charter-Pricing.html\" class=\"text-white text-primary\">Yacht Charter Pricing</a></li>\n                </ul>\n            </div>\n            <div class=\"col-12 col-md-6 col-lg-3\">\n                <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style mb-2\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\"><a href=\"index.html\" class=\"text-primary\">Quick links</a></h5>\n                <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline mbr-article>\n                    <li class=\"mbr-text item-wrap\"><a href=\"Exclusive-Boat-Aruba-VIP-About.html\" class=\"text-white text-primary\">About&nbsp;</a>Zeñorita Charters</li><li class=\"mbr-text item-wrap\"><a href=\"Gallery-Exclusive-Boat-Aruba.html\" class=\"text-white text-primary\">Yacht Charter Gallery</a></li><li class=\"mbr-text item-wrap\"><a href=\"aruba-yacht-charter-faq.html\" class=\"text-white\">Yacht Charter FAQs</a></li><li class=\"mbr-text item-wrap\"><a href=\"index.html\" class=\"text-white text-primary\" target=\"_blank\">Google Reviews</a>\n</li><li class=\"mbr-text item-wrap\"><a href=\"COVID-19-Exclusive-Boat-Aruba.html\" class=\"text-white text-primary\">COVID-19 Statement</a></li><li class=\"mbr-text item-wrap\">\n</li><li class=\"mbr-text item-wrap\">\n</li><li class=\"mbr-text item-wrap\">\n</li>\n                </ul>\n            </div>\n            <div class=\"col-12 mt-4\" mbr-if=\"showCopyright\">\n                <p class=\"mbr-text mb-0 mbr-fonts-style copyright align-center\" mbr-theme-style=\"display-4\" data-app-selector=\".copyright\" data-app-placeholder=\"Type Text\">\