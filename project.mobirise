              "padding-left": "5px",
              "padding-right": "5px",
              "margin-top": "0",
              "margin-right": "0",
              "margin-bottom": "0",
              "margin-left": "0",
              "line-height": "1"
            },
            "& when not (@collapsed)": {
              ".navbar-toggler:not(:disabled):not(.disabled)": {
